{
  "plugins": ["prettier-plugin-go-template", "prettier-plugin-tailwindcss"],
  "htmlWhitespaceSensitivity": "css",
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "go-template"
      }
    }
  ]
}
